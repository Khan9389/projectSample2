<script>
$(function(){
	var tab = window.location.search;
	tab = "tab"+ tab.substr(5);
	changeTab(tab);
	
	performanceYear();
	
	//getPerformanceInfo();
	//getPartnerInfo();
})
function changeTab(val){
	$('#' + val + '').prop("checked", true)
}

function performanceYear(){
	params = {
	}
	cf_call("/performance/selectYear", params, selectYearCB);
}
function selectYearCB(data){
	console.log(data);
	$("#searchYear").html("");
	var listDivHtml = "";
	for(var i=0; i<data.length; i++){
		listDivHtml += '<option value="' + data[i].YEAR + '">' + data[i].YEAR + '</option>';
	}
	$("#searchYear").html(listDivHtml);
	getPerformanceInfo();
}

function getPerformanceInfo(){
	cv_pageInfo.func = getPerformanceInfo;
	var pageInfo = {
			pageNo : cv_pageInfo.pageNo,
			orders : [
				{target : "START_DATE", isAsc : false},
				{target : "END_DATE", isAsc : false},
				{target : "CONTENT", isAsc : false},
			],
			limit : 10
	}
	
	params = {
		searchYear : $("#searchYear").val().trim()
	}
	cf_callPage("/performance/getList", params, pageInfo, goSearchCB);
}
function goSearchCB(data){
	console.log(data);
	g_dataList = data.list;
	$("#performanceBox").html("");
	var listDivHtml = "";
	
 	for(var i=0; i<g_dataList.length; i++){
		listDivHtml += '<dl class="total_history_box01">';
		if(g_dataList[i].STATUS == "ing"){
			listDivHtml += '	<dt class="contents_year00"><span class="recruitment01_first01_content01">.</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' 
								+ g_dataList[i].START_DATE.substring(2,4) + '년 ' + g_dataList[i].START_DATE.substring(5,7) + '월' + ' ~ 진행중</dt>';
		}else{
			listDivHtml += '	<dt class="contents_year00"><span class="recruitment01_first01_content01">.</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' 
								+ g_dataList[i].START_DATE.substring(2,4) + '년 ' + g_dataList[i].START_DATE.substring(5,7) + '월' + ' ~ ' 
								+ g_dataList[i].END_DATE.substring(2,4) + '년 ' + g_dataList[i].END_DATE.substring(5,7) + '월' + '</dt>';
		}
		listDivHtml += '	<dd class="contents_history00">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' + g_dataList[i].CONTENT + '</dd>';
		listDivHtml += '</dl>';
	}
	$("#performanceBox").html(listDivHtml);
}
</script>
</html>